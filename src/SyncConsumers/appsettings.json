{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "SyncOptions": {
    "Host": "fides-rabbitmq.germanywestcentral.azurecontainer.io",
    "VirtualHost": "/",
    "Endpoint": "fides",
    "RetryLimit": 6,
    "MinIntervalInSeconds": 0,
    "MaxIntervalInSeconds": 60,
    "IntervalDeltaInSeconds": 5,
    "MessageLimit": 100,
    "PrefetchCount": 100,
    "ConcurrencyLimit": 1,
    "TimeLimit": 10,
    "PriorityLimit": 2
  },
  "ELKOptions": {
    "ElasticSearchUri": "http://fides-elk.germanywestcentral.azurecontainer.io:9200",
    "DefaultIndex": "fides",
    "MaximumRetries": 6,
    "MaxRetryTimeoutInSeconds": 60
  },
  "ConnectionStrings": {
    "KeyVaultUri": "https://fides-keyvault.vault.azure.net/"
  }
}
